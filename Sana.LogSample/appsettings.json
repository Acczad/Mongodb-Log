{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "LogDataStore": {
      "CnnString": "mongodb://username:password@127.0.0.1:27017",
      "DatabaseName": "logDB",
      "ErrorCollectionName": "ErrDocs",
      "InformationCollectionName": "InfoDocs"
    }
  },
  "AllowedHosts": "*"
}
